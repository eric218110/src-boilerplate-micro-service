{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": [
        "prettier",
        "@typescript-eslint"
    ],
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}